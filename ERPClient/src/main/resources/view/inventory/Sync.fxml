<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXNodesList?>
<?import com.jfoenix.controls.JFXTabPane?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="830.0" style="-fx-background-color: #f6f6f7;" stylesheets="@../../css/sync.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.viewcontroller.InventoryStaff.InventorySyncController">
    <children>
        <VBox fx:id="vBox" alignment="TOP_CENTER" prefHeight="555.0" prefWidth="814.0" spacing="20.0" style="-fx-background-color: #f6f6f7;">
            <children>
                <HBox alignment="CENTER_LEFT" maxWidth="840.0" minHeight="81.0" minWidth="716.0" prefHeight="81.0" prefWidth="716.0" style="-fx-background-color: white; -fx-background-radius: 10; -fx-border-radius: 10;">
                    <effect>
                        <DropShadow color="#00000058" height="15.07" radius="9.1975" width="23.72" />
                    </effect>
                    <children>
                        <Rectangle fill="#1db1a4" height="40.0" stroke="TRANSPARENT" strokeType="INSIDE" width="8.0" />
                        <Label fx:id="title" text="同步库存">
                            <font>
                                <Font name="System Bold" size="24.0" />
                            </font>
                            <padding>
                                <Insets left="10.0" />
                            </padding>
                            <HBox.margin>
                                <Insets />
                            </HBox.margin>
                        </Label>
                        <Label fx:id="addIcon" onMouseClicked="#clickAddButton" styleClass="icon-label" text="+">
                            <HBox.margin>
                                <Insets left="15.0" />
                            </HBox.margin>
                        </Label>
                        <JFXNodesList fx:id="TypeChooser" alignment="CENTER" onMouseExited="#closeTypeChooser" rotate="90.0" spacing="50.0">
                            <children>
                                <JFXButton onMouseClicked="#clickAddOverflow" prefWidth="70.0" rotate="270.0" style="-fx-border-color: #1db1a4; -fx-border-radius: 10; -fx-border-width: 2; -fx-background-color: #ffffff; -fx-background-radius: 10;" text="报溢单" textFill="#1db1a4">
                                    <font>
                                        <Font name="Microsoft YaHei Bold" size="12.0" />
                                    </font>
                                </JFXButton>
                                <JFXButton onMouseClicked="#clickAddLoss" prefWidth="70.0" rotate="270.0" style="-fx-border-color: #1db1a4; -fx-border-radius: 10; -fx-border-width: 2; -fx-background-color: #ffffff; -fx-background-radius: 10;" text="报损单" textFill="#1db1a4">
                                    <font>
                                        <Font name="Microsoft YaHei Bold" size="12.0" />
                                    </font>
                                </JFXButton>
                            </children>
                            <HBox.margin>
                                <Insets left="60.0" />
                            </HBox.margin>
                        </JFXNodesList>
                    </children>
                    <opaqueInsets>
                        <Insets />
                    </opaqueInsets>
                    <padding>
                        <Insets left="20.0" />
                    </padding>
                    <VBox.margin>
                        <Insets bottom="10.0" />
                    </VBox.margin>
                </HBox>
            <JFXTabPane fx:id="tabPane" />
            </children>
            <padding>
                <Insets bottom="20.0" left="30.0" right="20.0" top="10.0" />
            </padding>
        </VBox>
    </children>
</AnchorPane>
